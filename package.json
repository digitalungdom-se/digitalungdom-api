    "test": "NODE_ENV=test jest",
    "prebuild": "npm run lint -s",
    "build": "ttsc",
    "prestart": "npm run build -s",
    "start": "node build/index.js",
    "start-dev": "ttsc -w & nodemon build/index.js",
    "build-prod": "ttsc",
    "docker": "docker-compose up --build --force-recreate --remove-orphans"
